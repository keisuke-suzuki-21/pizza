<% @page_title = "オーダー一覧(発送前)" %>
<h1><%= @page_title %></h1>

<% if @orders.present? %>
  <% @orders.each do |order| %>
    <% if order.member.present? %>
      <p><%= order.member.name + "さんの注文"%></p>
    <% else %>
      <p><%= "未ログインの人の注文"%></p>
    <% end %>
    <p><%= order.time %></p>
    <p><%= link_to "注文詳細", [:edit, :admin, order] %></p>
  <% end %>
<% else %>
  <p>注文履歴がありません。</p>
<% end %>

<div><%= link_to "<<戻る", admin_administrators_path %></div>
