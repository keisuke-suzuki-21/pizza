<% @page_title = "注文履歴(発送前)" %>
<h1><%= @page_title %></h1>

<% if @orders.present? %>
  <% @orders.each do |order| %>
    <p><%= order.member.name + "さんの注文"%></p>
    <p><%= order.address %></p>
  <% end %>
<% else %>
  <p>注文履歴がありません。</p>
<% end %>

<div><%= link_to "<<戻る", admin_administrators_path %></div>
