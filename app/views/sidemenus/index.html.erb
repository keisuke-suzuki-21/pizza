<% @page_title = "サイドメニュー一覧" %>
<h1><%= @page_title %></h1>

<div class="toolbar"><%= link_to "追加", new_sidemenu_path %></div>

<% if @sidemenus.present? %>
  <table class="list">
    <thead>
      <tr>
        <th>サイドメニュー名</th>
      </tr>
    </thead>
    <tbody>
      <% @sidemenus.each do |sidemenu| %>
        <tr>
          <td><%= link_to sidemenu.name, sidemenu %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <p>サイドメニュー情報がありません。</p>
<% end %>
